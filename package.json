{
  "name": "takehome",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"npm run start-watch\" \"npm run wp-server\"",
    "next-dev": "next dev",
    "next-build": "next build",
    "next-start": "next start",
    "next-lint": "next lint",
    "hasura-start": "cd crudio && ./go.sh",
    "hasura-stop": "cd crudio && ./stop.sh",
    "relay-compiler": "mkdir __generated__ && relay-compiler --watch",
    "relay-schema": "gq http://localhost:6789/v1beta1/relay -H \"X-Hasura-Admin-Secret: crudio\" --introspect > relay-schema.graphql"
  },
  "dependencies": {
    "@ionic/core": "^6.2.7",
    "next": "12.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-relay": "^14.1.0",
    "relay-runtime": "^14.1.0"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^10.1.0",
    "@types/node": "18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/react-relay": "^14.1.1",
    "concurrently": "^7.4.0",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.0",
    "graphqurl": "^1.0.1",
    "relay-compiler": "^14.1.0",
    "typescript": "4.8.3"
  }
}
